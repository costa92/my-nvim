{
  "snippets.ultisnips.pythonPrompt": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true,
        "completeUnimported": true,
        "staticcheck": true,
        "analyses": {
          "unusedparams": true,
          "shadow": true
        }
      }
    }
  },
  "go.goplsOptions": {
    "staticcheck": true,
    "completeUnimported": true,
    "usePlaceholders": true,
    "analyses": {
      "unusedparams": true,
      "shadow": true
    }
  }
}